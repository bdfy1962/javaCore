+package Arrays;
+
+public class FirstArraysTask {
+
+
+    public static boolean isSuccess(int number) {
+        return number % 2 == 0;
+    }
+
+    public static int getSuccessFulNumbersCount(int[] values) {
+        int[] numbers = new int[5];
+        numbers[0] = 9;
+        numbers[1] = 8;
+        numbers[2] = 7;
+        numbers[3] = 6;
+        numbers[4] = 5;
+        int completeResult = 0;
+        for (int value : values) {
+            if (FirstArraysTask.isSuccess(value)) {
+                completeResult++;
+            }
+        }
+        return completeResult;
+    }
+    }
